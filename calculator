#add mult sub
choice = int(input("""
Enter choice:
1- add
2- multiply
3- divide

choice: """))
num1 = int(input("Enter number 1: "))
num2 = int(input("Enter number 2: "))

def add(num1,num2):
    return num1 + num2
def mult(num1,num2):
    return num1 * num2
def div(num1,num2):
    return num1 / num2

if choice == 1:
    print(add(num1, num2))
elif choice == 2:
    print(mult(num1, num2))
elif choice == 3:
    print(div(num1, num2))
    
